{
  "private": true,
  "name": "type-jam",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "jest": "^28.1.1",
    "jest-preset-preact": "^1.0.0",
    "node-sass": "^6.0.1",
    "preact-cli": "^3.3.5",
    "sass-loader": "^10.2.1",
    "sirv-cli": "1.0.3",
    "typescript": "^4.7.3",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "dependencies": {
    "nes.css": "^2.3.0",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
